  vertical-align: middle;
  transform: perspective(1px) translateZ(0);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0);
  transition-duration: 0.3s;
  transition-property: border-radius;
  btn-cmn-setting01()
}
.hvr-round-corners a:hover, .hvr-round-corners a:focus, .hvr-round-corners a:active {
  border-radius: 1em;
}


/* SHADOW/GLOW TRANSITIONS */
/* Glow */
.hvr-glow a {
  display: inline-block;
  vertical-align: middle;
  transform: perspective(1px) translateZ(0);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0);
  transition-duration: 0.3s;
  transition-property: box-shadow;
  btn-cmn-setting01()
}
.hvr-glow a:hover, .hvr-glow a:focus, .hvr-glow a:active {
  box-shadow: 0 0 8px rgba(0, 0, 0, 0.6);
}

/* Shadow */
.hvr-shadow a {
  display: inline-block;
  vertical-align: middle;
  transform: perspective(1px) translateZ(0);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0);
  transition-duration: 0.3s;
  transition-property: box-shadow;
  btn-cmn-setting01()
}
.hvr-shadow a:hover, .hvr-shadow a:focus, .hvr-shadow a:active {
  box-shadow: 0 10px 10px -10px rgba(0, 0, 0, 0.5);
}

/* Grow Shadow */
.hvr-grow-shadow a {
  display: inline-block;
  vertical-align: middle;
  transform: perspective(1px) translateZ(0);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0);
  transition-duration: 0.3s;
  transition-property: box-shadow, transform;
  btn-cmn-setting01()
}
.hvr-grow-shadow a:hover, .hvr-grow-shadow a:focus, .hvr-grow-shadow a:active {
  box-shadow: 0 10px 10px -10px rgba(0, 0, 0, 0.5);
  transform: scale(1.1);
}

/* Box Shadow Outset */
.hvr-box-shadow-outset a {
  display: inline-block;
