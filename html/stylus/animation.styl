@charset 'utf-8'

[class^="hvr-"] {
  display inline-block;
  margin-right 5px
  margin-bottom 5px
}

btn-cmn-setting01()
  display inline-block
  padding 1em
  cursor pointer
  background #e1e1e1
  color #666


/* 2D TRANSITIONS */
/* Grow */
.hvr-grow a {
  display: inline-block;
  vertical-align: middle;
  transform: perspective(1px) translateZ(0);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0);
  transition-duration: 0.3s;
  transition-property: transform;
  btn-cmn-setting01()
}
.hvr-grow a:hover, .hvr-grow a:focus, .hvr-grow a:active {
  transform: scale(1.1);
}

/* Shrink */
.hvr-shrink a {
  display: inline-block;
  vertical-align: middle;
  transform: perspective(1px) translateZ(0);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0);
  transition-duration: 0.3s;
  transition-property: transform;
  transition-property: transform;
  btn-cmn-setting01()
}
.hvr-shrink a:hover, .hvr-shrink a:focus, .hvr-shrink a:active {
  transform: scale(0.9);
}

/* Pulse */
@keyframes hvr-pulse {
  25% {
    transform: scale(1.1);
  }
  75% {
    transform: scale(0.9);
  }
}
.hvr-pulse a {
  display: inline-block;
  vertical-align: middle;
  transform: perspective(1px) translateZ(0);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0);
  btn-cmn-setting01()
}
.hvr-pulse a:hover, .hvr-pulse a:focus, .hvr-pulse a:active {
  animation-name: hvr-pulse;
  animation-duration: 1s;
  animation-timing-function: linear;
  animation-iteration-count: infinite;
}

/* Pulse Grow */
@keyframes hvr-pulse-grow {
  to {
    transform: scale(1.1);
  }
}
.hvr-pulse-grow a {
  display: inline-block;
  vertical-align: middle;
  transform: perspective(1px) translateZ(0);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0);
  btn-cmn-setting01()
}
.hvr-pulse-grow a:hover, .hvr-pulse-grow a:focus, .hvr-pulse-grow a:active {
  animation-name: hvr-pulse-grow;
  animation-duration: 0.3s;
  animation-timing-function: linear;
  animation-iteration-count: infinite;
  animation-direction: alternate;
}

/* Pulse Shrink */
@keyframes hvr-pulse-shrink {
  to {
    transform: scale(0.9);
  }
}
.hvr-pulse-shrink a {
  display: inline-block;
  vertical-align: middle;
  transform: perspective(1px) translateZ(0);
