button,
input,
optgroup,
select,
textarea
  margin 0
  font-size 100%
  font-family inherit
  line-height 1.15

img,
video,
iframe
  max-width 100%
  height auto

b,
strong
  font-weight bolder

ol,
ul
  list-style none

time
  display block

img
  border-style none
  vertical-align top
  border none
  max-width 100%
  height auto
img:not([src*="."])
  display none

button,
[role="button"]
  cursor pointer

table
  border-collapse collapse
th
  font-weight normal

.img img
  margin-right auto
  margin-left auto

.map
  position relative
  overflow hidden
  &:after
    display block
    padding-top 50%
    content ""
  iframe
    position absolute
    top 0
    right 0
    left 0
    margin auto
    max-width initial
    width 100%
    height 100%

a,
button
  color inherit
  text-decoration none
  transition 0.4s ease
  &:hover
    opacity 0.6
    text-decoration none
    color inherit
    cursor pointer
  &[href*="tel"]
    pointer-events none
  @media (max-width 1024px)
    &[href*="tel"]
      pointer-events initial !important

.wrapper
  overflow hidden
  min-width 320px
[data-element-id] .wrapper
  padding-top 0 !important
  opacity 1

/* inner */
.inner
  margin 0 auto
  max-width 1000px
  width 92%
.inner600
  margin 0 auto
  max-width 600px
  width 92%
.inner700
  margin 0 auto
  max-width 700px
  width 92%
.inner800
