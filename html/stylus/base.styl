h2,
h3,
h4,
h5,
h6
  color inherit
  font-weight inherit
  font-size inherit
  font-family inherit
  line-height inherit

button,
input,
optgroup,
select,
textarea
  padding 0
  color inherit
  line-height inherit

img,
svg,
video,
canvas,
audio,
iframe,
embed,
object
