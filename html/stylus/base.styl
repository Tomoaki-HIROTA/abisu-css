/* カラム */
.up-col2
  display flex
  flex-wrap wrap
  > *
    width 50%
.up-col3
  display flex
  flex-wrap wrap
  > *
    width 33.333%
.up-col4
  display flex
  flex-wrap wrap
  > *
    width 25%
.up-col5
  display flex
  flex-wrap wrap
  > *
    width 20%
.up-col6
  display flex
  flex-wrap wrap
  > *
    width 16.666%
for gapy in range(0,100,10)
  .up-gapy{gapy}
    > *
      margin-bottom gapy px

for gapx in range(1,10,1)
  .up-gapx{gapx}p
    margin-left -(gapx/2)%
    margin-right -(gapx/2)%
    > *
      margin-left (ceil((gapx/2),3))%
      margin-right (ceil((gapx/2),3))%
for gapx in range(1,10,1)
  .up-col1.up-gapx{gapx}p
    > *
      width (100 - gapx)%
  .up-col2.up-gapx{gapx}p
    > *
      width (50 - gapx)%
  .up-col3.up-gapx{gapx}p
    > *
      width (33.333 - gapx)%
  .up-col4.up-gapx{gapx}p
    > *
      width (25 - gapx)%
  .up-col5.up-gapx{gapx}p
    > *
      width (20 - gapx)%
  .up-col6.up-gapx{gapx}p
    > *
      width (16.666 - gapx)%

for gapx-px in range(0,30,5)
  .up-gapx{gapx-px}
    margin-left -(gapx-px/2)px
    margin-right -(gapx-px/2)px
    > *
      margin-left (ceil((gapx-px/2),3))px
      margin-right (ceil((gapx-px/2),3))px
for gapx-px in range(0,30,5)
  .up-col1.up-gapx{gapx-px}
    > *
      width "calc(100% - %spx)" % (gapx-px)
  .up-col2.up-gapx{gapx-px}
    > *
      width "calc(50% - %spx)" % (gapx-px)
  .up-col3.up-gapx{gapx-px}
    > *
      width "calc(33.333% - %spx)" % (gapx-px)
  .up-col4.up-gapx{gapx-px}
    > *
      width "calc(25% - %spx)" % (gapx-px)
  .up-col5.up-gapx{gapx-px}
    > *
      width "calc(20% - %spx)" % (gapx-px)
  .up-col6.up-gapx{gapx-px}
    > *
      width "calc(16.666% - %spx)" % (gapx-px)

