[class*="heightLine"]
  display block

/* フィード */
.fead-mv
  opacity 0
  &.mv
    opacity 1
    transition 0.5s ease-out
.fead-up
  opacity 0
  transform translateY(50px)
  &.mv
    opacity 1
    transform translateY(0)
    transition 0.5s ease-out
.fead-right
  opacity 0
  transform translateX(-20%)
  &.mv
    opacity 1
    transform translateX(0)
    transition 1s
.fead-left
  opacity 0
  transform translateX(20%)
  &.mv
    opacity 1
    transform translateX(0)
    transition 1s


